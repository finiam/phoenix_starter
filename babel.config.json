{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["react-imported-component/babel"],
  "env": {
    "development": {
      "plugins": [
        "@babel/plugin-transform-react-jsx-source",
        "react-refresh/babel"
      ]
    }
  }
}
